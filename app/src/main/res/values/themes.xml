
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.FundBank" parent="Theme.Material3.Light.NoActionBar">
        <item name="colorPrimary">@color/blue_600</item>
        <item name="colorPrimaryVariant">@color/blue_800</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/purple_500</item>
        <item name="colorSecondaryVariant">@color/purple_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="android:statusBarColor">@color/blue_800</item>
    </style>
    <style name="BottomNavigationTheme" parent="Widget.Material3.BottomNavigationView">
        <item name="itemActiveIndicatorStyle">@null</item>
    </style>
    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="searchHintIcon">@null</item>
        <item name="queryBackground">@android:color/transparent</item>
        <item name="submitBackground">@android:color/transparent</item>
    </style>
    <style name="SplashTheme" parent="Theme.FundBank">
        <item name="android:windowBackground">@color/blue_600</item>
        <item name="android:statusBarColor">@color/blue_600</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>

</resources>
